{
  "name": "modanterist",
  "version": "1.0.0",
  "description": "a pinterest clone in react + redux",
  "main": "server/server.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "5.11.0"
  },
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/karma start karma.config.js",
    "test:dev": "npm run test -- --watch",
    "clean": "rm -rf dist",
    "webpack-prod": "NODE_ENV=production webpack --progress -p",
    "server-prod": "NODE_ENV=production node server/server.js",
    "dev": "npm run clean && NODE_ENV=development webpack --progress && nodemon server/server.js",
    "start": "npm run clean && npm run webpack-prod && npm run test && npm run server-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModanBrothers/modanterist.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "ModanBrothers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ModanBrothers/modanterist/issues"
  },
  "homepage": "https://github.com/ModanBrothers/modanterist#readme",
  "dependencies": {
    "autoprefixer": "^6.3.3",
